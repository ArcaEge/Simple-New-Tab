{
  "name": "Simple New Tab",
  "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
  "version": "1.2",
  "manifest_version": 3,
  "permissions": ["storage", "downloads", "search"],
  "chrome_url_overrides": {
    "newtab": "NewTab.html"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
